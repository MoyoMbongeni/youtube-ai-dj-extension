{
"manifest_version": 3,
"name": "My AI DJ",
"version": "1.0",
"description": "Uses AI to curate the perfect YouTube playlist based on your vibe.",
"icons": {
"128": "icon.png"
},
"action": {
"default_popup": "popup.html",
"default_title": "Open AI DJ",
"default_icon": "icon.png"
},
"permissions": [
"identity"
],
"oauth2": {
"client_id": "YOUR_OAUTH_CLIENT_ID.apps.googleusercontent.com",
"scopes": [
"https://www.googleapis.com/auth/youtube"
]
}
}